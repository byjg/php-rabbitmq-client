  "autoload": {
    "psr-4": {
      "ByJG\\MessageQueueClient\\RabbitMQ\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1 <8.5",
